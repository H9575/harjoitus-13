#include <iostream>
#include <string>
using namespace std;

int main()
{
	string etunimi, sukunimi, kokonimi;

	cout << "Etunimi: ";getline(cin, etunimi);
	cout << "Sukunimi: ";getline(cin, sukunimi);

	kokonimi = etunimi +" "+ sukunimi;

	cout << "Kokonimi: " << kokonimi << endl;
}
